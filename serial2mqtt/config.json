{
  "name": "serial2mqtt",
  "version": "1.3",
  "slug": "serial2mqtt",
  "description": "gateway serial <-> mqtt",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "auto_uart": true,
  "options": {
    "uart": "/dev/ttyUSB1",
    "baud": 57600,
	"mqtt_server": "core-mosquitto",
	"mqtt_user": "",
	"mqtt_pass": "",
	"mqtt_topic": "serial2mqtt",
	"debug": false
  },
  "schema": {
    "uart": "str",
    "baud": "int",
	"mqtt_server": "str",
	"mqtt_user": "str",
	"mqtt_pass": "str",
	"mqtt_topic": "str",
	"debug": "bool"
  }
}
